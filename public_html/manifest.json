{
    "name": "Bait Blocker",
    "description": "Remove content from Youtube depending on blacklisted sentences.",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["tabs", "storage"],
    "icons": {
        "16": "icon16.png",
        "128": "icon128.png"
    }
}